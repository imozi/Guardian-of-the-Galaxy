{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch ssr.ts --watch src/server --exec 'ts-node ssr.ts'",
    "build": "yarn clear && yarn run build:client && yarn run build:server && yarn build:ssr",
    "build:client": "vite build --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx",
    "build:ssr": "tsc -p tsconfig.server.json",
    "serve": "cross-env NODE_ENV=production node dist/ssr.js",
    "clear": "yarn rimraf ./dist",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest ./"
  },
  "dependencies": {
    "axios": "1.2.0",
    "compression": "^1.7.4",
    "cross-fetch": "3.1.5",
    "express": "^4.18.2",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.3",
    "sirv": "^2.0.2",
    "standardized-audio-context": "25.3.37",
    "standardized-audio-context-mock": "9.6.9",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@testing-library/react": "13.3.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.16",
    "@types/jest": "28.1.8",
    "@types/node": "^18.11.18",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "@vitejs/plugin-react": "2.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.0.3",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lefthook": "1.1.1",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
    "rimraf": "^4.1.2",
    "sass": "1.56.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.3",
    "vite": "3.0.7"
  },
  "license": "MIT"
}
